#include <iostream>

using namespace std;

int main() {
    int n;
    int cnt=0;
    cin>>n;
    //500,100,50,10
    int cointypes={500,100,50,10}
    
    while(1)
    {
        if(n>=500)
        {
            n-=500;
            cnt++;
        }
         if(n<500&&n>=100)
        {
            n-=100;
            cnt++;
        }
          if(n<100&&n>=50)
        {
            n-=50;
            cnt++;
        }
          if(n<50&&n>=10)
        {
            n-=10;
            cnt++;
        }
        if(n==0)
            break;
        
    }
    
    cout<<cnt;
    
  /*  for(int i=0;i<4;i++)
    {
        int coin=cointypes[i];
        cnt+=n/coin;
        n%=coin;
    }*/
}
